{
  "name": "deducer",
  "version": "0.2.0",
  "description": "Apply a transfomration map to reshape and process your data.",
  "keywords": [
    "deduce",
    "transform",
    "process",
    "composable",
    "unpack",
    "reshape",
    "transpose"
  ],
  "main": "dist/lib",
  "module": "dist/deducer.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Robert Pitt <robertpitt1988@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertpitt/deducer.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist docs",
    "build": "tsc && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "tsc -p tsconfig.json -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": 100
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/invariant": "^2.2.31",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/object-path": "^0.11.0",
    "colors": "^1.4.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-config": "^25.2.4",
    "lint-staged": "^10.1.0",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "object-path": "^0.11.4",
    "pipe-and-compose": "0.0.6"
  }
}
